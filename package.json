{
    "name": "gatsby-starter-default",
    "private": true,
    "description": "A simple starter to get up and developing quickly with Gatsby",
    "version": "0.1.0",
    "author": "Kyle Mathews <mathews.kyle@gmail.com>",
    "dependencies": {
        "gatsby": "^2.0.76",
        "gatsby-image": "^2.0.20",
        "gatsby-plugin-manifest": "^2.0.9",
        "gatsby-plugin-no-sourcemaps": "^2.0.1",
        "gatsby-plugin-offline": "^2.0.16",
        "gatsby-plugin-react-helmet": "^3.0.2",
        "gatsby-plugin-sass": "^2.0.7",
        "gatsby-plugin-sharp": "^2.0.14",
        "gatsby-remark-autolink-headers": "^2.0.12",
        "gatsby-remark-copy-linked-files": "^2.0.8",
        "gatsby-remark-images": "^3.0.1",
        "gatsby-remark-prismjs": "^3.2.0",
        "gatsby-remark-responsive-iframe": "^2.0.8",
        "gatsby-remark-smartypants": "^2.0.7",
        "gatsby-source-filesystem": "^2.0.12",
        "gatsby-transformer-remark": "^2.1.19",
        "gatsby-transformer-sharp": "^2.1.8",
        "lodash": "^4.17.11",
        "node-sass": "^4.11.0",
        "prismjs": "^1.15.0",
        "prop-types": "^15.6.2",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-helmet": "^5.2.0"
    },
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "scripts": {
        "build": "npx rimraf public && gatsby build",
        "dev": "gatsby develop",
        "start": "npm run develop",
        "format": "prettier --write \"src/**/*.js\"",
        "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
        "lint-staged": "npx lint-staged"
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "prettier --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "post-commit": "git update-index --again",
            "pre-commit": "npm run lint-staged"
        }
    },
    "devDependencies": {
        "husky": "^1.3.1",
        "lint-staged": "^8.1.0",
        "prettier": "^1.15.2",
        "rimraf": "^2.6.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gatsbyjs/gatsby-starter-default"
    },
    "bugs": {
        "url": "https://github.com/gatsbyjs/gatsby/issues"
    }
}
